{
  "manifest_version": 2,
  "name": "JIRA Assist",
  "version": "0.0.0.1",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "img/logo-32.png",
    "36": "img/logo-36.png",
    "48": "img/logo-48.png",
    "78": "img/logo-78.png",
    "96": "img/logo-96.png",
    "128": "img/logo-128.png"
  },
  "permissions": [
    "<all_urls>", "storage"
  ],
  "app": {
    "background": {
      "scripts": [
        "js/libs/jquery-1.11.0.min.js",
        "js/libs/moment.min.js",
        "js/libs/underscore-min.js",
        "js/libs/backbone-min.js",
        "js/models.js",
        "js/jira.js",
        "js/app.js"
      ]
    }
  }
}